  agent any
  stages {
    stage('Build') {
      steps {
        bat 'echo "First Step initialzed"'
      }
    }
    stage('Test') {
      steps {
        bat 'echo "Testing is being here"'
      }
    }
    stage('deploye') {
      steps {
        bat 'echo "Deploy"'
      }
    }
  }
}